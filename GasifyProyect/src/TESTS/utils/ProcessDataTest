import org.junit.jupiter.api.Assertions;
import org.junit.jupiter.api.BeforeEach;
import org.junit.jupiter.api.Test;

import java.util.List;

public class ProcessDataTest {

    private ProcessData processData;

    @BeforeEach
    public void setup() {
        processData = new ProcessData();

    }

    @Test
    public void allUsage_ValidInput_ReturnsListOfString() {

        String customerID = "123";
        String firstDate = "01/01/2023";
        String secondDate = "31/01/2023";

        List<String> usageList = processData.allUsage(customerID, firstDate, secondDate);

        Assertions.assertNotNull(usageList);

    }

    @Test
    public void findSpecificMonthBill_ValidInput_ReturnsListOfBilling() {

        String customerId = "123";
        String date = "01/2023";

        List<Billing> billList = processData.findSpecificMonthBill(customerId, date);

        Assertions.assertNotNull(billList);

    }


}

